{
  "name": "davatar",
  "type": "module",
  "version": "2.7.5",
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite dev --mode mock",
    "build": "rimraf dist/assets/* && vite build",
    "serve": "vite preview",
    "lint": "eslint --cache  src",
    "bump": "bumpp package.json --commit \"release: v\" --push --tag",
    "up:deps": "taze major",
    "prepare": "npx simple-git-hooks",
    "up:polyfill": "nolyfill install",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/components": "^13.3.0",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.7.9",
    "hoci": "^0.6.0",
    "pinia": "^2.3.0",
    "query-string": "^9.1.1",
    "tslx": "^0.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "workbox-expiration": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@curev/eslint-config": "^0.3.7",
    "@fourze/core": "^0.30.2",
    "@fourze/vite": "^0.30.2",
    "@iconify-json/ic": "^1.2.2",
    "@types/node": "^20.17.12",
    "@unocss/preset-icons": "^0.65.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "bumpp": "^9.10.0",
    "eslint": "^8.57.1",
    "less": "^4.2.1",
    "lint-staged": "^15.3.0",
    "nolyfill": "^1.0.43",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sass": "^1.83.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.18.0",
    "typescript": "^5.7.3",
    "unocss": "^0.65.4",
    "unocss-preset-palette": "^0.3.6",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.21.1",
    "vue-tsc": "^2.2.0",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1.0.28"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npx eslint --fix --cache"
    ]
  }
}
