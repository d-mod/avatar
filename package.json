{
  "name": "davatar",
  "type": "module",
  "version": "2.7.5",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite dev --mode mock",
    "build": "rimraf dist/assets/* && vite build",
    "serve": "vite preview",
    "lint": "eslint --cache  src",
    "bump": "bumpp package.json --commit \"release: v\" --push --tag",
    "up:deps": "taze major",
    "prepare": "npx simple-git-hooks",
    "up:polyfill": "nolyfill install",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/components": "^13.9.0",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "hoci": "^0.8.1",
    "pinia": "^3.0.3",
    "query-string": "^9.3.1",
    "tslx": "^0.1.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "workbox-expiration": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@curev/eslint-config": "^0.6.0",
    "@fourze/core": "^0.31.3",
    "@fourze/vite": "^0.31.3",
    "@iconify-json/ic": "^1.2.4",
    "@types/node": "^20.19.23",
    "@unocss/eslint-plugin": "^66.5.4",
    "@unocss/preset-icons": "^66.5.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "less": "^4.4.2",
    "lint-staged": "^16.2.6",
    "nolyfill": "^1.0.44",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.8.1",
    "typescript": "^5.9.3",
    "unocss": "^66.5.4",
    "unocss-preset-palette": "^0.4.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.4.1",
    "vite-plugin-pwa": "^1.1.0",
    "vue-tsc": "^2.2.12",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1.0.44"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npx eslint --fix --cache"
    ]
  }
}
